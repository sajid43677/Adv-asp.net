

<main>
    <ul class="nav bg-black bg-opacity-25">
        <li class="nav-item">
            <a class="nav-link " href="../Home/Index">Index</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="../Home/Education">Education</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="../Home/Personal">Personal</a>
        </li>
        <li class="nav-item">
            <a class="nav-link " href="../Home/Projects">Projects</a>
        </li>
        <li class="nav-item">
            <a class="nav-link " href="../Home/References">References</a>
        </li>
    </ul>
    <section class="row" aria-labelledby="aspnetTitle">
        <h1 id="title">References</h1>
    </section>
    <div>
        <table class="table">
            <thead class="thead-light">
                <tr>
                   
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                  
                </tr>
            </thead>
            <tbody>
                @foreach (var i in Model.References)
                {
                    <tr>
                       
                        <td>@i.Name</td>
                        <td>@i.Email</td>
                       
                    </tr>
                }
            </tbody>
        </table>
    </div>


</main>



